@model GetCarCountByFuelTypeDto

    <div class="col-md-6 col-xl-3">
        <div class="card bg-success border-success">
            <div class="card-body">
                <div class="mb-4">
                    <span class="badge badge-soft-light float-right">@ViewBag.FuelTypes</span>
                    <h5 class="card-title mb-0 text-white">Car Count By Fuel Types</h5>
                </div>
                <div class="row d-flex align-items-center mb-4">
                    <div class="col-8">
                        <h2 class="d-flex align-items-center text-white mb-0">
                            <i class="fas fa-gas-pump mr-2"></i>
                            @Model.CarCount
                        </h2>
                    </div>
                </div>
            </div>
        </div>
    </div>