<!DOCTYPE html>
<html lang="en">
<head>
    <title>Carbook @(string.IsNullOrEmpty(ViewData["Title"]?.ToString()) ? "" : $" - {ViewData["Title"]}")</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    @* Bootstrap Icons *@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">

    @* Sweet Alert **@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.17.2/dist/sweetalert2.all.min.js"></script>

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="~/website-template/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="~/website-template/css/animate.css">

    <link rel="stylesheet" href="~/website-template/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/website-template/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/website-template/css/magnific-popup.css">

    <link rel="stylesheet" href="~/website-template/css/aos.css">

    <link rel="stylesheet" href="~/website-template/css/ionicons.min.css">

    <link rel="stylesheet" href="~/website-template/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/website-template/css/jquery.timepicker.css">


    <link rel="stylesheet" href="~/website-template/css/flaticon.css">
    <link rel="stylesheet" href="~/website-template/css/icomoon.css">
    <link rel="stylesheet" href="~/website-template/css/style.css">
</head>
<body>
    <!-- Navbar -->
    <partial name="~/Views/Shared/PartialViews/_Navbar.cshtml" />
    <!-- END - Navbar -->

    @RenderBody()

    <!-- Footer -->
    @await Component.InvokeAsync("Footer")
    <!-- END - Footer-->
    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


    <script src="~/website-template/js/jquery.min.js"></script>
    <script src="~/website-template/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/website-template/js/popper.min.js"></script>
    <script src="~/website-template/js/bootstrap.min.js"></script>
    <script src="~/website-template/js/jquery.easing.1.3.js"></script>
    <script src="~/website-template/js/jquery.waypoints.min.js"></script>
    <script src="~/website-template/js/jquery.stellar.min.js"></script>
    <script src="~/website-template/js/owl.carousel.min.js"></script>
    <script src="~/website-template/js/jquery.magnific-popup.min.js"></script>
    <script src="~/website-template/js/aos.js"></script>
    <script src="~/website-template/js/jquery.animateNumber.min.js"></script>
    <script src="~/website-template/js/bootstrap-datepicker.js"></script>
    <script src="~/website-template/js/jquery.timepicker.min.js"></script>
    <script src="~/website-template/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/website-template/js/google-map.js"></script>
    <script src="~/website-template/js/main.js"></script>

    <script>
        $(document).ready(function () {
            $('#term-button').click(() => {
                Swal.fire({
                    title: 'Term and Conditions',
                    text: 'This is the term and conditions',
                    icon: 'info',
                    confirmButtonText: 'Ok'
                });

                console.log('Term and Conditions');
            });
        });
    </script>
</body>
</html>